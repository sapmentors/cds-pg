{
  "scripts": {
    "start": "cds run",
    "clear": "rm -rf _gen/*; mkdir _gen; cp as-scp-package.json _gen/package.json",
    "copycds:db": "cp -r db _gen/",
    "copycds:srv": "copyfiles srv/**/* _gen"
  },
  "dependencies": {
    "@sap/cds": "^5",
    "@sap/cds-dk": "^4",
    "@sap/xsenv": "^3.3.2",
    "@sap/xssec": "^3.2.13",
    "cds-pg": "*",
    "express": "^4.18.1",
    "passport": "^0.6.0",
    "pg": "^8.7.3"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "database"
      },
      "database": {
        "impl": "cds-pg",
        "model": ["srv"]
      }
    }
  }
}
